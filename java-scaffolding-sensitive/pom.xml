<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <groupId>com.test</groupId>
        <artifactId>java_scaffolding</artifactId>
        <version>1.0.0-SNAPSHOT</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>
    <artifactId>java-scaffolding-sensitive</artifactId>
    <version>1.0.0-SNAPSHOT</version>

    <properties>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

        <hutool-all.version>5.8.35</hutool-all.version>
        <java-scaffolding-common.version>1.0.0-SNAPSHOT</java-scaffolding-common.version>
        <com.fasterxml.jackson.core.version>2.15.0</com.fasterxml.jackson.core.version>
    </properties>

    <description>
        java-scaffolding-sensitive敏感信息處理
    </description>

    <dependencies>
        <!-- hutool version -->
        <!-- https://mvnrepository.com/artifact/cn.hutool/hutool-all/5.8.35 -->
        <dependency>
            <groupId>cn.hutool</groupId>
            <artifactId>hutool-all</artifactId>
            <version>${hutool-all.version}</version>
        </dependency>
        <!-- Core Utils (exp. String Utils) -->
        <dependency>
            <groupId>com.test</groupId>
            <artifactId>java-scaffolding-common</artifactId>
            <version>${java-scaffolding-common.version}</version>
        </dependency>
        <!-- Adding annotation for desensitized with annotation -->
        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-annotations</artifactId>
            <version>${com.fasterxml.jackson.core.version}</version>
        </dependency>
        <!-- Adding databind for desensitized with annotation -->
        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-databind</artifactId>
            <version>${com.fasterxml.jackson.core.version}</version>
        </dependency>

    </dependencies>

</project>